{
    "name": "@phaserjs/phaser-nano",
    "version": "1.0.0",
    "description": "Phaser 4 Nano",
    "scripts": {
        "prepublishOnly": "echo \"Error: Don't run 'npm publish' in root. Use 'npm run dist' instead.\" && exit 1",
        "build": "rollup -c rollup.config.js",
        "watch": "tsc --watch --pretty",
        "dist": "npm run build && cd dist && npm publish --ignore-scripts",
        "dist-examples": "rollup -c examples.dist.rollup.config.js",
        "build-examples": "rollup -c examples.rollup.config.js",
        "watch-examples": "rollup --watch -c examples.rollup.config.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phaserjs/phaser-nano.git"
    },
    "keywords": [
        "Phaser"
    ],
    "author": "Richard Davey <rich@photonstorm.com> (http://www.photonstorm.com)",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/phaserjs/phaser-nano/issues"
    },
    "homepage": "https://github.com/phaserjs/phaser-nano#readme",
    "devDependencies": {
        "rollup": "^2.1.0",
        "rollup-plugin-banner": "^0.2.1",
        "rollup-plugin-clear": "^2.0.7",
        "rollup-plugin-command": "^1.1.2",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-filesize": "^6.2.1",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^5.3.0",
        "rollup-plugin-typescript2": "^0.26.0",
        "rollup-plugin-visualizer": "^3.3.2",
        "typescript": "^3.8.3",
        "uplot": "^1.0.2"
    }
}
